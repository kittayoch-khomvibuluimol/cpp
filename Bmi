#include <iostream>
using namespace std;

int main() {
    double he,we,bmi;
    cout << "Bmi " << endl;
    cout << "Plase enter your weight" << endl;
    cin >> we;
    cout << "Plase enter your height CM" << endl;
    cin >> he;
    if(he>0 && we>0){
        double heM = he/100;
        double heMI = heM*heM;
        double bmi = we/heMI;
        cout << "Bmi " << we/heMI << endl;
        
    }else{
        cout << "invalid data plase try again" << endl;
    }if(bmi>=18.5 && bmi<=22.9){
        cout << "Normal" << endl;
    }else if(bmi>=23 && bmi<=24.9){
        cout << "Over weight" << endl;
    }else if(bmi>=25 && bmi<=29.9){
        cout << "Grade 1 obesity" << endl;
    }else if(bmi>=30 && bmi<=34.9){
        cout << "Grade 2 obesity" << endl;
    }else if(bmi>=35){
        cout << "Grade 3 obesity" << endl;
    }else {
        cout << "Underweight" << endl;
    }
    

    return 0;
}
